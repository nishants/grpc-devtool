{
  "name": "grpc-devtool.ui",
  "version": "0.0.10",
  "description": "UI for GrpcDevtool",
  "watch": {
    "build": {
      "patterns": [
        "ui"
      ],
      "extensions": "js,scss",
      "quiet": false,
      "runOnChangeOnly": false
    }
  },
  "scripts": {
    "build:webpack": "webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production npm run build:webpack",
    "build": "cross-env NODE_ENV=development npm run build:webpack",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "build:watch": "npm-watch",
    "lint:ui": "eslint ui",
    "test:react": "yarn jest --config ui/jest.react.config.js ./ui"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.8.3",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.3",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^5.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "antd": "^4.13.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  }
}
